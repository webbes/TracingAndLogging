{
  "compilerOptions": {
    "alwaysStrict": true,
    "baseUrl": "./",
    "declaration": true,
    "importHelpers": true,
    "module": "amd",
    "noEmitHelpers": true,
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "paths": {
      "AppInsights": [ "node_modules/applicationinsights-js/JavaScript/JavaScriptSDK.Module/AppInsightsModule.js" ],
      "tslib": [ "node_modules/tslib/tslib.d.ts" ]
    },
    "removeComments": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "target": "es5"
  }
}